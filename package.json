{
  "name": "vlogology",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "tailwind": "yarn tailwind:normalize && yarn tailwind:css",
    "tailwind:normalize": "tailwind build styles/normalize.src.css -c tailwind.config.js -o styles/normalize.css",
    "tailwind:css": "tailwind build styles/tailwind.src.css -c tailwind.config.js -o styles/tailwind.css",
    "css": "npm run tailwind && npm run purgecss",
    "purgecss": "purgecss -c purgecss.config.js -o src"
  },
  "dependencies": {
    "@types/next": "^9.0.0",
    "@types/react": "^16.9.53",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "next": "9.5.5",
    "prettier": "^2.1.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.27.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.4.0",
    "@types/node": "^14.14.3",
    "@zeit/next-css": "^1.0.1",
    "next-purgecss": "^4.0.0",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^1.9.6",
    "typescript": "^4.0.3"
  }
}
