    "next": "^10.0.0",
    "prettier": "^2.1.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-transition-group": "^4.4.1",
    "sass": "^1.27.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@svgr/webpack": "^5.4.0",
    "@types/node": "^14.14.5",
    "@types/styled-components": "^5.1.4",
    "@types/react-transition-group": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
