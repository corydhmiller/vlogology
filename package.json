{
  "name": "vlogology",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "tailwind": "yarn tailwind:normalize && yarn tailwind:css",
    "tailwind:normalize": "tailwind build styles/normalize.src.css -c tailwind.config.js -o styles/normalize.css",
    "tailwind:css": "tailwind build styles/tailwind.src.css -c tailwind.config.js -o styles/tailwind.css",
    "css": "npm run tailwind && npm run postcss",
    "postcss": "postcss -c postcss.config.js -o src"
  },
  "dependencies": {
    "@types/next": "^9.0.0",
    "@types/react": "^16.9.53",
    "@zeit/next-sass": "^1.0.1",
